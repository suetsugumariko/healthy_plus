<h1>Admin::Customers#edit</h1>
<p>Find me in app/views/admin/customers/edit.html.erb</p>
<h2>会員情報編集</h2>
<%= form_with model: @customer do |f| %>

  <h4>会員氏名</h4>
  <%= f.text_field :last_name %><%= f.text_field :first_name %>

  <h4>メールアドレス</h4>
  <%= f.email_field :email %>

  <h4>会員ステータス</h4>




  <%= f.submit '編集' %>

<% end %>