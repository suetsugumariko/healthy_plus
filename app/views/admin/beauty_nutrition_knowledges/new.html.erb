
<h2>美容栄養豆知識</h2>

<%= form_with model: @beauty_nutrition_knowledge, url: ' /admin/beauty_nutrition_knowledges', method: :post do |f| %>
  <h4>タイトル</h4>
  <%= f.text_field :title %>

  <h4>ジャンル名</h4>
  <%= f.text_field :genre_id %>

  <h4>内容</h4>
  <%= f.text_area :contrnts %>

  <%= f.submit '投稿' %>
<% end %>