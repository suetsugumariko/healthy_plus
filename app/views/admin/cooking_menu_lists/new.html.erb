<h1>Admin::CookingMenuList#new</h1>
<p>Find me in app/views/admin/cooking_menu_list/new.html.erb</p>
<h2>食事メニュー新規投稿</h2>

<%= form_with model: @cooking_menu_list, url: '/admin/cooking_menu_lists', method: :post  do |f| %>
  <h4>タイトル</h4>
  <%= f.text_field :title %>

  <h4>何人前？</h4>
  <%= f.text_field :for_how_meny_people %>人前

  <h4>材料</h4>
  <%= f.text_field :material %>

  <h4>作り方</h4>
  <%= f.text_area :way_of_making %>

  <%= f.submit '投稿' %>
<% end %>